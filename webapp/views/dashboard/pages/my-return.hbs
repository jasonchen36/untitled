{{>dashboard-left-sidebar isChat=false isUpload=false }}
{{#if true}}
    <section id="dashboard-my-return" class="small-7 medium-6 full-height overflow-y-auto">
            <div id="container-upload-explained">
                {{#debug}}{{/debug}}
                {{#compare taxReturns.0.status.id 5 operator="<"}}
                    <h1>Your Taxes Are In Progress</h1>
                    <p class="side-info-blurb">Thank you for submitting your tax documents to TAXplan.
                        {{#compare taxReturns.length 1 operator=">"}}Your returns are now in progress. {{else}}Your return is now in progress.{{/compare}}
                        Should we have any questions or notice that you’ve missed something your assigned TAXpro will be in touch through the message board.  We guarantee completion of tax preparation within 48 hours or by the next business day – provided we have everything we need from you to prepare the best possible return.</p>
                {{else compare taxReturns.0.status.id 7 operator="=="}}
                    <h1>YOUR RESULTS ARE IN!</h1>
                    <p class="side-info-blurb"><span class="capitalize">{{users.0.firstName}}</span>, by clicking on details you can see a summary of your tax results. Once payment to TAXplan is made you’ll be able to access a full copy of your tax return for yourself and your other filers.</p>
                {{else compare taxReturns.0.status.id 8 operator="=="}}
                    <h1>One Last Step!</h1>
                    <p class="side-info-blurb">In order for TAXplan to electronically file your tax return results with the CRA we need your authorization to do so. Just sign below where indicated and let us do the rest.</p>
                {{/compare}}
            </div>
    {{else}}
    <section id="dashboard-my-return" class="small-7 medium-8 full-height overflow-y-auto">
        <div class="border-bottom-dark-grey">
            <div class="i--icon-green-check"></div>
            <h1 class="display-inline-block font-header-size">Payment Successful</h1>
            <p id="label-upload-instructions" class="display-inline-block side-info-blurb label-instruction-header add-margin-left">A receipt has been emailed to your email address.</p>
            <button id="tax-return-submit" class="purple header long">Print Receipt</button>
        </div>
    {{/if}}

        <!--IN PROGRESS PAGE-->
    {{#if true}}
            <div class="i--icon-return-machine"></div>
        <!--RESULTS PAGE-->
    {{else if false}}
        <div id="container-return-line-items">
            <div class="return-line-item border-bottom-light-gray border-top-light-gray">
                <p class="return-line-item-name display-inline-block">Carmela Doctor</p>
                <p class="return-line-item-description display-inline-block text-upper {{#if isOwing}}font-red{{else}}font-green{{/if}}">Refund</p>
                <p class="return-line-item-description display-inline-block text-right {{#if isOwing}}font-red{{else}}font-green{{/if}}">$1234.50</p>
                <button id="quote-details" type="submit" class="green-outline button-details">Details</button>
            </div>
            <div class="return-line-item border-bottom-light-gray">
                <p class="return-line-item-name display-inline-block">Doug Doctor</p>
                <p class="return-line-item-description display-inline-block text-upper {{#if isOwing}}font-red{{else}}font-green{{/if}}">Taxes Owing</p>
                <p class="return-line-item-description display-inline-block text-right {{#if isOwing}}font-red{{else}}font-green{{/if}}">$1234.50</p>
                <button id="quote-details" type="submit" class="green-outline button-details">Details</button>
            </div>
            <div class="return-line-item border-bottom-light-gray">
                <p class="return-line-item-name display-inline-block">Skippy Doctor</p>
                <p class="return-line-item-description display-inline-block text-upper {{#if isOwing}}font-red{{else}}font-green{{/if}}">Refund</p>
                <p class="return-line-item-description display-inline-block text-right {{#if isOwing}}font-red{{else}}font-green{{/if}}">$1234.50</p>
                <button id="quote-details" type="submit" class="green-outline button-details">Details</button>
            </div>
            <div class="return-line-item border-bottom-light-gray">
                <p class="return-line-item-name display-inline-block">Other Doctor</p>
                <p class="return-line-item-description display-inline-block text-upper {{#if isOwing}}font-red{{else}}font-green{{/if}}">Refund Doctor</p>
                <p class="return-line-item-description display-inline-block text-right {{#if isOwing}}font-red{{else}}font-green{{/if}}">$1234.50</p>
                <button id="quote-details" type="submit" class="green-outline button-details">Details</button>
            </div>
        </div>
        <div class="text-center add-bottom-margin">
            <h1>Your Tax Preparation Cost is: $191.00</h1>
            <button class="green long">Pay TAXplan & E-file</button>
        </div>
        <!--PAY TAXPLAN PAGE-->
    {{else if false}}
        <div id="container-pay-line-items">
            <div class="line"></div>
            <div class="return-line-item">
                <p class="pay-line-item-name display-inline-block float-left">Tax Preparation - Carmela Doctor</p>
                <div class="i--icon-green-check float-right"></div>
                <p class="pay-line-item-name display-inline-block float-right text-right">$89.00</p>
            </div>
            <div class="line"></div>
            <div class="return-line-item">
                <p class="pay-line-item-name display-inline-block float-left">Tax Preparation - Doug Doctor</p>
                <div class="i--icon-green-check float-right"></div>
                <p class="pay-line-item-name display-inline-block float-right text-right">$89.00</p>
            </div>
            <div class="line"></div>
            <div class="return-line-item">
                <p class="pay-line-item-name display-inline-block float-left">Direct Deposit</p>
                <div class="i--icon-green-check float-right"></div>
                <p class="pay-line-item-name display-inline-block float-right text-right">$5.00</p>
            </div>
            <div class="line"></div>
            <div class="return-line-item">
                <p class="pay-line-item-name display-inline-block float-left">Tax</p>
                <div class="i--icon-green-check float-right"></div>
                <p class="pay-line-item-name display-inline-block float-right text-right">$8.00</p>
            </div>
            <div class="line"></div>
        </div>
        <div class="text-center add-bottom-margin">
            <h1 class="label-total">$191.00</h1>
            <button class="green long">Pay TAXplan & E-file</button>
        </div>
        <!--SIGNATURES PAGE-->
    {{else if false}}
            <div>
                <h1 class="text-left add-top-margin add-top-padding">One Last Step! Now we E-File.</h1>
                <p class="side-info-blurb">Your 2015 Return has been emailed to your email address. Sign below to authorize TAXplan to E-File your taxes with the CRA.</p>
            </div>
        <div>
            {{>dashboard-signature-tile}}
            {{>dashboard-signature-tile}}
            {{>dashboard-signature-tile}}
            {{>dashboard-signature-tile}}
        </div>
    {{/if}}
</section>

{{#if false}}
    {{>dashboard-right-sidebar hasDescription="true"}}
{{else}}
    {{>dashboard-right-sidebar}}
{{/if}}