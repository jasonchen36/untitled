<section id="dashboard-upload">
    <h1>upload</h1>
    <p>drop files here to upload</p>
    <p>or</p>
    <form id="dashboard-upload-form">
        <input id="dashboard-upload-input" type="file" name="files[]" data-url="/dashboard/upload">
        <button id="dashboard-upload-select" type="button">Select Files</button>
        <button id="dashboard-upload-submit" type="submit" class="disabled">Upload</button>
        <button id="dashboard-upload-cancel" type="reset" class="disabled">Cancel</button>
        <div id="dashboard-upload-progress"></div>
    </form>
</section>
