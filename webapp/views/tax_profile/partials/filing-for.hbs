<section id="tax-profile-filing-for" class="grid-container">
    <h1>filing for</h1>
    <p>
        {{account.name}}, please tell us who you are filing for. Select all that apply
    </p>
    <form id="filing-for-form">
        <p>
        <input type="checkbox" id="filing-for-myself" name="myself" {{#compare account.filingType.myself 1 operator="=="}}checked="checked"{{/compare}}/>
            <label for="filing-for-myself">Myself</label>
        </p>
        <p>
            <input type="checkbox" id="filing-for-spouse" name="spouse" {{#compare account.filingType.spouse 1 operator="=="}}checked="checked"{{/compare}}/>
            <label for="filing-for-spouse">My Spouse/Partner</label>
        </p>
        <p>
            <input type="checkbox" id="filing-for-other" name="other" {{#compare account.filingType.other 1 operator="=="}}checked="checked"{{/compare}}/>
            <label for="filing-for-other">Other Person/People</label>
        </p>
        <button type="submit" id="filing-for-submit">Next</button>
    </form>
    <button id="filing-for-back">Back</button>
</section>